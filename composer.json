{
    "name": "kishor-rajbanshi/chimpvine-test-pj",
    "description": "The test project for onboarding in chimpvine",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Kishor Rajbanshi",
            "email": "kishorrajbanshi00@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "vlucas/phpdotenv": "5.4.x-dev"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "prefer-stable": true,
    "require-dev": {
        "fakerphp/faker": "^1.19"
    }
}
